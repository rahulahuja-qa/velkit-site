{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "api/builder/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/builder",
      "dest": "api/builder/index.py",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/builder.html",
      "dest": "/public/builder.html"
    },
    {
      "src": "/velkit-logo.png",
      "dest": "/public/velkit-logo.png"
    }
  ]
}